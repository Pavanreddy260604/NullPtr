{
    "version": 2,
    "functions": {
        "api/index.js": {
            "maxDuration": 30,
            "memory": 1024
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.js"
        }
    ],
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET,OPTIONS,POST,PUT,DELETE"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "x-second-space-secret,Content-Type,Authorization"
                }
            ]
        },
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET,OPTIONS,POST,PUT,DELETE"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "x-second-space-secret,Content-Type,Authorization"
                }
            ]
        }
    ]
}